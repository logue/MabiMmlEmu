{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "mabinogi-mml-player",
  "version": "3.2.1",
  "description": "Mabinogi MML Web Player",
  "type": "module",
  "keywords": [
    "Mabinogi",
    "MML",
    "music-macro-language",
    "midi",
    "webplayer",
    "smfplayer"
  ],
  "license": "MIT",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/MabiMmlEmu",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/MabiMmlEmu.git"
  },
  "bugs": {
    "url": "https://github.com/logue/MabiMmlEmu/issues"
  },
  "files": [
    "dist",
    "MSXSprint.sf2"
  ],
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.6.0",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/sf2synth": "^0.5.9",
    "@logue/smfplayer": "^0.5.3",
    "@popperjs/core": "^2.11.8",
    "@zip.js/zip.js": "^2.7.24",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "dseg": "^0.46.0",
    "query-string": "^8.1.0",
    "streamsaver": "^2.0.6"
  },
  "devDependencies": {
    "eslint": ">=8.46.0",
    "eslint-config-google": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.65.1",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "semver": "^7.5.2",
    "yaml": "^2.2.2"
  }
}
