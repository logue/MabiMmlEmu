{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "mabinogi-mml-player",
  "version": "3.1.0",
  "description": "Mabinogi MML Web Player",
  "type": "module",
  "keywords": [
    "Mabinogi",
    "MML",
    "music-macro-language",
    "midi",
    "webplayer",
    "smfplayer"
  ],
  "license": "MIT",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/MabiMmlEmu",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/MabiMmlEmu.git"
  },
  "bugs": {
    "url": "https://github.com/logue/MabiMmlEmu/issues"
  },
  "files": [
    "dist",
    "MSXSprint.sf2"
  ],
  "engines": {
    "node": ">=18.15.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.4.1",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/smfplayer": "^0.5.0",
    "@popperjs/core": "^2.11.6",
    "@zip.js/zip.js": "^2.6.78",
    "bootstrap": "^5.3.0-alpha1",
    "bootstrap-icons": "^1.10.3",
    "dseg": "^0.46.0",
    "query-string": "^8.1.0",
    "streamsaver": "^2.0.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "vite": "^4.1.4",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.5.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  }
}
