{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "mabinogi-mml-player",
  "version": "3.0.1",
  "description": "Mabinogi MML Web Player",
  "type": "module",
  "keywords": [
    "Mabinogi",
    "MML",
    "music-macro-language",
    "midi",
    "webplayer",
    "smfplayer"
  ],
  "license": "MIT",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/MabiMmlEmu",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/MabiMmlEmu.git"
  },
  "bugs": {
    "url": "https://github.com/logue/MabiMmlEmu/issues"
  },
  "files": [
    "dist",
    "MSXSprint.sf2"
  ],
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.2.4",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.204.0",
    "@aws-sdk/s3-request-presigner": "^3.204.0",
    "@aws-sdk/signature-v4-crt": "^3.201.0",
    "@logue/sf2synth": "^0.4.12",
    "@logue/smfplayer": "^0.4.3",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "5.2.2",
    "bootstrap-icons": "^1.9.1",
    "dseg": "^0.46.0",
    "encoding-japanese": "^2.0.0",
    "query-string": "^7.1.1",
    "querystring": "^0.2.1",
    "streamsaver": "^2.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.2",
    "eslint": "^8.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.55.0",
    "vite": "^3.2.2",
    "vite-plugin-banner": "^0.6.1",
    "vite-plugin-checker": "^0.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
